	<table class="table table-striped">
		<tr>
			<th>Id</th>
			<th>Acciones</th>
		</tr>				
		{{#each pois}}
			<tr>
				<td>{{id}}</td>
				<td><a href="/admin/pois/editar/{{id}}"><span class="glyphicon glyphicon-pencil" aria-hidden="true"></span></a>
